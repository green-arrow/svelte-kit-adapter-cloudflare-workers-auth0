{
  "name": "svelte-kit-adapter-cloudflare-workers-auth0",
  "version": "0.0.1",
  "description": "An adapter for SvelteKit to deploy to Cloudfare Workers, using Auth0 for authentication.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/green-arrow/svelte-kit-adapter-cloudflare-workers-auth0.git"
  },
  "type": "module",
  "exports": {
    "import": "./index.js"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "files",
    "index.d.ts"
  ],
  "scripts": {
    "format": "npm run check-format -- --write",
    "check-format": "prettier --check . --ignore-path .gitignore"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "keywords": [
    "svelte",
    "sveltekit",
    "cloudflare",
    "cloudflare workers",
    "auth0"
  ],
  "author": "Andrew Walton <andrew@greenarrow.me>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/green-arrow/svelte-kit-adapter-cloudflare-workers-auth0/issues"
  },
  "homepage": "https://github.com/green-arrow/svelte-kit-adapter-cloudflare-workers-auth0#readme",
  "devDependencies": {
    "prettier": "^2.3.2"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^1.0.0-next.158"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "cookie": "^0.4.1",
    "esbuild": "^0.12.23"
  }
}
